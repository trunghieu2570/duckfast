networks:
  postgres:
    driver: bridge
services:
  db:
    environment:
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres:14.1-alpine
    networks:
      postgres: {}
    ports:
    - published: 5432
      target: 5432
    restart: always
    volumes:
    - db:/var/lib/postgresql/data:rw
  duckfast.web:
    build:
      context: C:\Users\trunghieu\source\repos\DuckFast
      dockerfile: DuckFast.Web/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: duckfastweb
    networks:
      postgres: {}
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\trunghieu\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\trunghieu\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'
volumes:
  db:
    driver: local