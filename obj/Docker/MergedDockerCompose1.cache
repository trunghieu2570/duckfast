networks:
  postgres:
    driver: bridge
services:
  db:
    environment:
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres:14.1-alpine
    networks:
      postgres: {}
    ports:
    - published: 5432
      target: 5432
    restart: always
    volumes:
    - db:/var/lib/postgresql/data:rw
  duckfast.web:
    build:
      context: C:\Users\trunghieu\source\repos\DuckFast
      dockerfile: DuckFast.Web/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: DuckFast.Web
      target: base
    container_name: DuckFast.Web
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: duckfastweb:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/DuckFast.Web.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      postgres: {}
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\trunghieu\source\repos\DuckFast\DuckFast.Web:/app:rw
    - C:\Users\trunghieu\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\trunghieu\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\trunghieu\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\trunghieu\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\trunghieu\source\repos\DuckFast:/src:rw
version: '3.4'
volumes:
  db:
    driver: local